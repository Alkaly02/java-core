package models;

public class BibliothequeElement {
	private String titre, auteur;
	private int anneePublication;
	
	public BibliothequeElement(String titre, String auteur, int annee) {
		this.titre = titre;
		this.auteur = auteur;
		this.anneePublication = annee;
	}
	
	public void afficherDetails() {
		
	}
	
	public void setTitre(String titre) {
		this.titre = titre;
	}
	public String getTitre(String titre) {
		return this.titre;
	}
	
}
